{
  "name": "reunited-example",
  "version": "0.0.1",
  "description": "Example usage of Jest Webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run build && npm test",
    "build": "rm -rf __bundled_tests__ && node node_modules/.bin/webpack-cli build",
    "test": "node node_modules/.bin/jest",
    "build-debug": "node --inspect node_modules/.bin/webpack-cli build",
    "test-debug": "node --inspect node_modules/.bin/jest  --runInBand"
  },
  "engines": {
    "node": ">=5"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleLoader": "<rootDir>/node_modules/reunited/ModuleLoader",
    "testPathDirs": [
      "<rootDir>/__bundled_tests__"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__bundled_tests__/common.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/css-loader",
      "<rootDir>/node_modules/bundle-loader"
    ]
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.17.0",
    "glob": "^5.0.14",
    "jest-cli": "^0.7",
    "reunited": "file:../",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.3",
    "webpack": "5.33.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "react": "^0.13.3"
  }
}
